{
	"generationOrder": [
		"stats.abilityScores",

		"identity.pronouns",
		"identity.genderIdentity",
		"identity.genderExpression",
		"identity.sex",
		"identity.sexualOrientation",
		
		"stats.alignmentTendancies",
		"personality.alignmentMoral",
		
		"description.age",
		"description.height",
		"description.weight",

		"description.race",
		"description.subrace",

		"description.skin",
		"personality.deity",

		"occupation.type",
		"occupation.profession",

		"relationships",
		"hooks"
	],
	"fields": {
		"stats": {
			"abilityScores": {
				"default": {
					"strength": 9,
					"dexterity": 9,
					"constitution": 9,
					"intelligence": 9,
					"wisdom": 9,
					"charisma": 9
				}
			},
			"alignmentTendancies": {
				"default": {
					"ethical": {
						"positive": "{randInt:0,10}",
						"neutral": "{randInt:0,10}",
						"negative": "{randInt:0,10}"
					},
					"moral": {
						"positive": "{randInt:0,10}",
						"neutral": "{randInt:0,10}",
						"negative": "{randInt:0,10}"
					}
				}
			}
		},
		"identity": {
			"pronouns": {
				"default": {
					"singular": "",
					"thirdPerson": "",
					"possessive": ""
				},
				"value": "{roll:identity/pronouns}"
			},
			"genderIdentity": {
				"default": "{roll:identity/genderIdentity}"
			},
			"genderExpression": {
				"default": "{roll:identity/genderExpression}"
			},
			"sex": {
				"default": "{roll:identity/sex}"
			},
			"sexualOrientation": {
				"default": "{roll:identity/sexualOrientation}"
			}
		},
		"description": {
			"age": {
				"default": "{roll:traits/age}"
			},
			"height": {
				"default": "{roll:traits/height}"
			},
			"weight": {
				"default": "{roll:traits/weight}"
			},
			"race": {
				"default": "{roll:race}"
			},
			"subrace": {
				"value": "{roll:race/$(description.race)}"
			},
			"skin": {
				"value": "{roll:race/$(description.race)/skin}"
			}
		},
		"personality": {
			"alignmentMoral": {
				"default": "{roll:traits/alignmentMoral}"
			},
			"religion": {
				"value": "{roll:race/$(description.race)/deity}"
			}
		},
		"occupation": {
			"type": {
				"default": "TODO: class || job"
			},
			"profession": {
				"value": "TODO: selection for (class || job)"
			}
		},
		"relationships": {
			"default": []
		},
		"hooks": {
			"default": []
		}
	}
}