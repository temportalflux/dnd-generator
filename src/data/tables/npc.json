{
	"generationOrder": [
		"stats.abilityScores",
		"stats.alignmentTendancies",
		"description.height",
		"description.weight"
	],
	"fieldOrder": [
		"stats.abilityScores",
		"stats.alignmentTendancies",
		"personality.alignmentMoral",
		"identity.pronouns",
		"identity.genderIdentity",
		"identity.genderExpression",
		"identity.sex",
		"identity.sexualOrientation",
		"description.height",
		"description.weight",
		"description.age",
		"description.race",
		"description.subrace",
		"description.skin",
		"description.eyeColor",
		"description.face",
		"description.hair",
		"description.beard",
		"personality.deity",
		"identity.name",
		"identity.surname"
	],
	"fields": {
		"stats.abilityScores": {
			"stringify": "STR($(strength)) DEX($(dexterity)) CON($(constitution)) INT($(intelligence)) WIS($(wisdom)) CHA($(charisma))",
			"canReroll": false,
			"children": [
				"strength",
				"dexterity",
				"constitution",
				"intelligence",
				"wisdom",
				"charisma"
			]
		},
		"stats.abilityScores.strength": {
			"source": 9
		},
		"stats.abilityScores.dexterity": {
			"source": 9
		},
		"stats.abilityScores.constitution": {
			"source": 9
		},
		"stats.abilityScores.intelligence": {
			"source": 9
		},
		"stats.abilityScores.wisdom": {
			"source": 9
		},
		"stats.abilityScores.charisma": {
			"source": 9
		},
		"stats.alignmentTendancies": {
			"stringify": "$(ethical.positive)/$(ethical.neutral)/$(ethical.negative) - $(moral.positive)/$(moral.neutral)/$(moral.negative)",
			"children": [
				"ethical",
				"moral"
			]
		},
		"stats.alignmentTendancies.ethical": {
			"stringify": "$(positive)/$(neutral)/$(negative)",
			"children": [
				"positive",
				"neutral",
				"negative"
			]
		},
		"stats.alignmentTendancies.ethical.positive": {
			"source": "{roll:traits/alignment/tendancy}"
		},
		"stats.alignmentTendancies.ethical.neutral": {
			"source": "{roll:traits/alignment/tendancy}"
		},
		"stats.alignmentTendancies.ethical.negative": {
			"source": "{roll:traits/alignment/tendancy}"
		},
		"stats.alignmentTendancies.moral": {
			"stringify": "$(positive)/$(neutral)/$(negative)",
			"children": [
				"positive",
				"neutral",
				"negative"
			]
		},
		"stats.alignmentTendancies.moral.positive": {
			"source": "{roll:traits/alignment/tendancy}"
		},
		"stats.alignmentTendancies.moral.neutral": {
			"source": "{roll:traits/alignment/tendancy}"
		},
		"stats.alignmentTendancies.moral.negative": {
			"source": "{roll:traits/alignment/tendancy}"
		},
		"identity.name": {
			"source": "{roll:race/$(description.race)/name-$(identity.genderIdentity)}",
			"dependencies": [
				"description.race",
				"identity.genderIdentity"
			]
		},
		"identity.surname": {
			"source": "{roll:race/$(description.race)/surname}",
			"dependencies": [
				"description.race"
			]
		},
		"identity.pronouns": {
			"source": "{roll:identity/pronouns}",
			"stringify": "$(singular)/$(thirdPerson)/$(possessive)"
		},
		"identity.genderIdentity": {
			"source": "{roll:identity/genderIdentity}"
		},
		"identity.genderExpression": {
			"source": "{roll:identity/genderExpression}"
		},
		"identity.sex": {
			"source": "{roll:identity/sex}"
		},
		"identity.sexualOrientation": {
			"source": "{roll:identity/sexualOrientation}"
		},
		"description.age": {
			"source": "{roll:traits/age}"
		},
		"description.height": {
			"source": "{roll:traits/height}"
		},
		"description.weight": {
			"source": "{roll:traits/weight}"
		},
		"description.race": {
			"source": "{roll:race}"
		},
		"description.subrace": {
			"source": "{roll:race/$(description.race)}",
			"dependencies": [
				"description.race"
			]
		},
		"description.skin": {
			"source": "{roll:race/$(description.race)/skin}",
			"dependencies": [
				"description.race"
			]
		},
		"description.eyeColor": {
			"source": "{roll:traits/eyeColor}"
		},
		"description.face": {
			"source": "{roll:traits/face}"
		},
		"description.hair": {
			"source": "{roll:traits/$(description.race)/hair}",
			"dependencies": [
				"description.race"
			]
		},
		"description.beard": {
			"source": "{roll:traits/$(description.race)/beard}",
			"dependencies": [
				"description.race"
			]
		},
		"personality.alignmentMoral": {
			"source": "{roll:traits/alignmentMoral}"
		},
		"personality.deity": {
			"source": "{roll:race/$(description.race)/deity}",
			"dependencies": [
