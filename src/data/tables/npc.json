{
	"generationOrder": [
		"stats.abilityScores",
		
		"stats.alignmentTendancies",
		"personality.alignmentMoral",

		"description.height",
		"description.weight",

		"description.race",
		"description.subrace",

		"description.eyeColor",
		"description.skin",
		"description.face",
		"description.hair",
		"description.beard"
	],
	"fieldOrder": [
		"stats.abilityScores",
		"stats.alignmentTendancies",
		"personality.alignmentMoral",
		"identity.pronouns",
		"identity.genderIdentity",
		"identity.genderExpression",
		"identity.sex",
		"identity.sexualOrientation",
		"description.height",
		"description.weight",
		"description.age",
		"description.race",
		"description.subrace",
		"description.skin",
		"description.eyeColor",
		"description.face",
		"description.hair",
		"description.beard",
		"personality.deity",
		"identity.name",
		"identity.surname"
	],
	"fields": {
		"stats": {
			"abilityScores": {
				"stringify": "STR($(strength)) DEX($(dexterity)) CON($(constitution)) INT($(intelligence)) WIS($(wisdom)) CHA($(charisma))",
				"canReroll": false,
				"children": {
					"strength": {
						"source": 9
					},
					"dexterity": {
						"source": 9
					},
					"constitution": {
						"source": 9
					},
					"intelligence": {
						"source": 9
					},
					"wisdom": {
						"source": 9
					},
					"charisma": {
						"source": 9
					}
				}
			},
			"alignmentTendancies": {
				"stringify": "$(ethical.positive)/$(ethical.neutral)/$(ethical.negative) - $(moral.positive)/$(moral.neutral)/$(moral.negative)",
				"children": {
					"ethical": {
						"stringify": "$(positive)/$(neutral)/$(negative)",
						"children": {
							"positive": {
								"source": "{roll:traits/alignment/tendancy}"
							},
							"neutral": {
								"source": "{roll:traits/alignment/tendancy}"
							},
							"negative": {
								"source": "{roll:traits/alignment/tendancy}"
							}
						}
					},
					"moral": {
						"stringify": "$(positive)/$(neutral)/$(negative)",
						"children": {
							"positive": {
								"source": "{roll:traits/alignment/tendancy}"
							},
							"neutral": {
								"source": "{roll:traits/alignment/tendancy}"
							},
							"negative": {
								"source": "{roll:traits/alignment/tendancy}"
							}
						}
					}
				}
			}
		},
		"identity": {
			"name": {
				"source": "{roll:race/$(description.race)/name-$(identity.genderIdentity)}",
				"dependencies": [
					"description.race",
					"identity.genderIdentity"
				]
			},
			"surname": {
				"source": "{roll:race/$(description.race)/surname}",
				"dependencies": [
					"description.race"
				]
			},
			"pronouns": {
				"source": "{roll:identity/pronouns}",
				"stringify": "$(singular)/$(thirdPerson)/$(possessive)"
			},
			"genderIdentity": {
				"source": "{roll:identity/genderIdentity}"
			},
			"genderExpression": {
				"source": "{roll:identity/genderExpression}"
			},
			"sex": {
				"source": "{roll:identity/sex}"
			},
			"sexualOrientation": {
				"source": "{roll:identity/sexualOrientation}"
			}
		},
		"description": {
			"age": {
				"source": "{roll:traits/age}"
			},
			"height": {
				"source": "{roll:traits/height}"
			},
			"weight": {
				"source": "{roll:traits/weight}"
			},
			"race": {
				"source": "{roll:race}"
			},
			"subrace": {
				"source": "{roll:race/$(description.race)}",
				"dependencies": [
					"description.race"
				]
			},
			"skin": {
				"source": "{roll:race/$(description.race)/skin}",
				"dependencies": [
					"description.race"
				]
			},
			"eyeColor": {
				"source": "{roll:traits/eyecolor}"
			},
			"face": {
				"stringify": "$(shape) $(attractiveness)",
				"children": {
					"shape": {
						"source": "{roll:traits/face/shape}"
					},
					"attractiveness": {
						"source": "{roll:traits/face/attractiveness}",
						"stringify": "$(attractiveness.modifier || '') $(attractiveness.quality)"
					}
				}
			},
			"hair": {
				"source": "{roll:race/$(description.race)/hair}",
				"dependencies": [
					"description.race"
				]
			},
			"beard": {
				"source": "{roll:race/$(description.race)/beard}",
				"dependencies": [
					"description.race"
				]
			}
		},
		"personality": {
			"alignmentMoral": {
				"source": "{roll:traits/alignmentMoral}"
			},
			"deity": {
				"source": "{roll:race/$(description.race)/deity}",
				"dependencies": [
					"description.race"
				]
			},
			"quirks": {
				"source": "{roll:race/$(description.race)/quirks}",
				"dependencies": [
					"description.race"
				]
			}
		},
		"occupation": {
			"type": {
				"source": "{roll:occupation/type}"
			},
			"profession": {
				"source": "{roll:occupation/$(occupation.type)}",
				"dependencies": [
					"occupation.type"
				]
			}
		},
		"relationships": {},
		"hooks": {}
	}
}