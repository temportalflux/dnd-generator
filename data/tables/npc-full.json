{
	"generationOrder": [
		"stats.abilityScores",
		"description.height",
		"description.weight",

		"stats.alignmentTendancies",
		"personality.alignmentMoral",

		"description.race",
		"description.subrace",
		"description.eyeColor",
		"description.skin",
		"description.face",
		"description.hair",
		"description.beard",

		"personality.deity",
		
		"identity.pronouns",
		"identity.genderIdentity",
		"identity.genderExpression",
		"identity.sex",
		"identity.sexualOrientation",

		"identity.name",
		"description.age",

		"occupation.type",
		"occupation.profession",

		"TODO hooks",
		"TODO traits",
		"TODO special physical",
		"TODO quirks",
		"personality.quirks",







		"relationships",
		"hooks"
	],
	"fields": {
		"stats": {
			"abilityScores": {
				"default": {
					"strength": 9,
					"dexterity": 9,
					"constitution": 9,
					"intelligence": 9,
					"wisdom": 9,
					"charisma": 9
				}
			},
			"alignmentTendancies": {
				"default": {
					"ethical": {
						"positive": "{roll:traits/alignment/tendancy}",
						"neutral": "{roll:traits/alignment/tendancy}",
						"negative": "{roll:traits/alignment/tendancy}"
					},
					"moral": {
						"positive": "{roll:traits/alignment/tendancy}",
						"neutral": "{roll:traits/alignment/tendancy}",
						"negative": "{roll:traits/alignment/tendancy}"
					}
				}
			}
		},
		"identity": {
			"name": {
				"value": [
					"{roll:race/$(description.race)/name-$(identity.genderIdentity)}",
					"{roll:race/$(description.race)/surname}"
				]
			},
			"pronouns": {
				"default": {
					"singular": "",
					"thirdPerson": "",
					"possessive": ""
				},
				"value": "{roll:identity/pronouns}"
			},
			"genderIdentity": {
				"default": "{roll:identity/genderIdentity}"
			},
			"genderExpression": {
				"default": "{roll:identity/genderExpression}"
			},
			"sex": {
				"default": "{roll:identity/sex}"
			},
			"sexualOrientation": {
				"default": "{roll:identity/sexualOrientation}"
			}
		},
		"description": {
			"age": {
				"default": "{roll:traits/age}"
			},
			"height": {
				"default": "{roll:traits/height}"
			},
			"weight": {
				"default": "{roll:traits/weight}"
			},
			"race": {
				"default": "{roll:race}"
			},
			"subrace": {
				"value": "{roll:race/$(description.race)}"
			},
			"skin": {
				"value": "{roll:race/$(description.race)/skin}"
			},
			"eyeColor": {
				"value": "{roll:traits/eyeColor}"
			},
			"face": {
				"value": "{roll:traits/face}"
			},
			"hair": {
				"value": "{roll:traits/$(description.race)/hair}"
			},
			"beard": {
				"value": "{roll:traits/$(description.race)/beard}"
			}
		},
		"personality": {
			"alignmentMoral": {
				"default": "{roll:traits/alignmentMoral}"
			},
			"deity": {
				"value": "{roll:race/$(description.race)/deity}"
			},
			"quirks": {
				"default": [],
				"value": [
					"{roll:race/$(description.race)/quirks}"
				]
			}
		},
		"occupation": {
			"type": {
				"default": "{roll:occupation/type}"
			},
			"profession": {
				"value": "{roll:occupation/$(occupation.type)}"
			}
		},
		"relationships": {
			"default": []
		},
		"hooks": {
			"default": []
		}
	}
}